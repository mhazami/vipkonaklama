@using Radyn.Web.Mvc;
@model Radyn.ContentManager.DataStructure.Content
@{
    ViewBag.Title = @Resources.Common.Detail + " " + @Resources.ContentManager.Content;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Head{


}
@section PageTitle
{
    @Resources.Common.Detail @Resources.ContentManager.Content
}
@section PageAction
{
<div class="button-div m-btn waves-red i-return">
    @{Html.Radyn().ImageActionLink().ActionName("Index").ControllerName("Content").Area("ContentManager").Text(@Resources.Common.Return).Align(ImageActionLink.Alignment.Left).ImageUrl(Url.Content("~/Content/Images/go-back.png")).ImagePosition(ImageActionLink.Position.Right).Render();}
</div>
}
@section PageActionDown
{
<div class="button-div m-btn waves-red i-return">
    @{Html.Radyn().ImageActionLink().ActionName("Index").ControllerName("Content").Area("ContentManager").Text(@Resources.Common.Return).Align(ImageActionLink.Alignment.Left).ImageUrl(Url.Content("~/Content/Images/go-back.png")).ImagePosition(ImageActionLink.Position.Right).Render();}
</div>

}
@using (Html.BeginForm())
{
    Html.RenderAction("GetDetails", "Content", new { area = "ContentManager", Id = ViewBag.Id });
}
