@using Radyn.Web.Mvc;
@using Radyn.WebApp.Areas.Common.Component
@model Radyn.ContentManager.DataStructure.MenuHtml
@{
    ViewBag.Title = @Resources.Common.Detail + " " + @Resources.ContentManager.Html;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Head{}
@section PageTitle
{
    @Resources.Common.Detail @Resources.ContentManager.Html
}
@section PageAction
{
<div class="button-div m-btn waves-red i-return">
    @{Html.Radyn().ImageActionLink().ActionName("Index").ControllerName("MenuHtml").Area("ContentManager").Text(@Resources.Common.Return).Align(ImageActionLink.Alignment.Left).ImageUrl(Url.Content("~/Content/Images/go-back.png")).ImagePosition(ImageActionLink.Position.Right).Render();}
</div>
}
@section PageActionDown
{

}
@using (Html.BeginForm())
{    
     Html.RenderAction("GetDetail","MenuHtml",new {area="ContentManager",Id=ViewBag.Id});
}
