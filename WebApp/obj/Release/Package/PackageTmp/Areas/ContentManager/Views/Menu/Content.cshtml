@{
    ViewBag.Title = Resources.ContentManager.ListOfMenus;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Head
{

    <script type="text/javascript">

        function GetMenus() {
            $.get("/ContentManager/Menu/GetMenu", { id: new Date().getTime() }, function (data) {
                if (data != "") {
                    $("#DiveMenu").hide();
                    $("#DiveMenu").html(data);
                    $("#DiveMenu").slideDown();
                }
            });
        }

        $(document).ready(function () {
            GetMenus();
        });
    </script>
}
<div>
    <div id="DiveMenu">
    </div>
    <div id="DiveMenuContents">
    </div>
    <div id="DiveContentShow">
    </div>
</div>

